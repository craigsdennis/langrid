<process
  name="bpelns:${processName}"
  location="${bpelFile}"
  xmlns="http://schemas.active-endpoints.com/pdd/2004/09/pdd.xsd"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2003/03/addressing"
  xmlns:bpelns="${bpelNamespace}"
  xmlns:gsp = "http://groovy.codehaus.org/2005/gsp"
>
