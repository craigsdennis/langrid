	  <partnerRole endpointReference="invoker" />
	<gsp:scriptlet>}</gsp:scriptlet>
	<gsp:scriptlet>if(partner.myRoleService){</gsp:scriptlet>
	  <myRole
		service="${partner.myRoleService}"
		allowedRoles=""
		binding="RPC"
	  />
	<gsp:scriptlet>}</gsp:scriptlet>
	</partnerLink>
	<gsp:scriptlet>}</gsp:scriptlet>
  </partnerLinks>

  <wsdlReferences>
	<gsp:scriptlet>for(wsdl in wsdlReferences){</gsp:scriptlet>
	<wsdl
	  namespace="${wsdl.namespace}"
	  location="${wsdl.file}"
	/>
	<gsp:scriptlet>}</gsp:scriptlet>
  </wsdlReferences>
</process>
